<?xml version="1.0" encoding="utf-8"?>
<manifest package="link.ebbinhaus.planning"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="link.ebbinghaus.planning.custom.other.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="link.ebbinghaus.planning.view.activity.impl.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="link.ebbinghaus.planning.view.activity.impl.PlanningBuildActivity">
        </activity>
    </application>

</manifest>
